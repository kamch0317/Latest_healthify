<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.HomeFragment">


    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="381dp"
        android:layout_height="164dp"
        android:layout_marginTop="192dp"
        android:alpha="0"
        android:scaleType="center"
        app:layout_constraintBottom_toBottomOf="@+id/workplan"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/bg_user"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/s_p_211yn_dark_blue__1_" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/foodplanrv"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="5dp"
        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="@+id/imageView10"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        tools:listitem="@layout/singlecolumn" />




    <ImageView
        android:id="@+id/bg_user"
        android:layout_width="300dp"
        android:layout_height="123dp"
        android:contentDescription="@string/user_bg"
        android:scaleType="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/home_bg" />

    <TextView
        android:id="@+id/user_name"
        android:layout_width="125dp"
        android:layout_height="41dp"
        android:fontFamily="@font/allerta"
        android:text="@string/username_user"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.129"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"

        app:layout_constraintVertical_bias="0.104" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="187dp"
        android:layout_height="38dp"
        android:fontFamily="@font/allerta"
        android:text="Welcome back "
        android:textColor="@color/white"
        android:textColorHighlight="@color/white"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/bg_user"
        app:layout_constraintEnd_toEndOf="@+id/bg_user"
        app:layout_constraintHorizontal_bias="0.298"
        app:layout_constraintStart_toStartOf="@+id/bg_user"
        app:layout_constraintTop_toTopOf="@+id/bg_user"
        app:layout_constraintVertical_bias="0.409" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/allerta"
        android:text="Work Plan"
        android:textColor="@color/darkblue3"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.052"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/bg_user"
        app:layout_constraintVertical_bias="0.219" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/allerta"
        android:text="Food Plan"
        android:textColor="@color/darkblue3"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.051"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/bg_user"
        app:layout_constraintVertical_bias="0.519" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="382dp"
        android:layout_height="151dp"
        android:layout_marginTop="228dp"
        android:alpha="0"
        android:scaleType="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.551"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/bg_user"
        app:layout_constraintVertical_bias="0.465"
        app:srcCompat="@drawable/s_p_211yn_dark_blue__1_" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/workplan"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView8"
        app:layout_constraintVertical_bias="1.0" />


</androidx.constraintlayout.widget.ConstraintLayout>